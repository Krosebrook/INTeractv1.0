{
  "name": "interact-ive-agent",
  "version": "1.0.0",
  "description": "Full-featured refactoring agent using Claude Agent SDK",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --loader ts-node/esm src/index.ts",
    "dev": "node --loader ts-node/esm --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "claude",
    "agent",
    "refactoring",
    "code-analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.6",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
